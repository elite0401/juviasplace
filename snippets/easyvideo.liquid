DO NOT EDIT. This snippet is generated automatically. Your changes may be overwritten.
{% endcomment %}
{% if template contains "product" or template contains "index" %}
  {% assign featured_url = product.featured_image | img_url: "1024x1024" | split: "_1024x1024" | first %}
  <meta id="easyvideo_featured_product_image" content="{{ featured_url }}" />
  {% assign mainImage = variant.featured_image | default: product.featured_image %}
  {% assign featured_url = mainImage | img_url: "1024x1024" | split: "_1024x1024" | first %}
  <meta id="easyvideo_featured_main_image" content="{{ featured_url }}" />
  <meta id="easyvideo_enabled" content="1" />
  <meta id="easyvideo_selector" content="" />
  <meta id="easyvideo_popup" content="1" />
  <script type="text/javascript"> $("img[alt*='//vimeo.com/']").css("opacity", ".2"); $("img[alt*='youtube.com/watch']").css("opacity", ".2"); $("img[alt*='youtu.be/']").css("opacity", ".2");</script>
  <style>.product-gallery .main a { display: block }</style>
{% endif %}